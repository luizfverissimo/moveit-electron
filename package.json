{
  "name": "moveit-electron",
  "version": "1.0.0",
  "main": "main.js",
  "repository": "https://github.com/luizfverissimo/moveit-electron.git",
  "author": "Luiz Fernando Ver√≠ssimo <62751844+luizfverissimo@users.noreply.github.com>",
  "description": "Time controller for physical actives, project created at NLW #4. Port to Electron.js",
  "license": "MIT",
  "scripts": {
    "start": "electron-forge start",
    "watch": "webpack --config webpack.common.js --watch",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "electron": "^12.0.9",
    "file-loader": "^6.2.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "js-cookie": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "assets/favicon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "moveit_electron"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
